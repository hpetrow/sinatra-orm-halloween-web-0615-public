<h1>All Houses</h1>

<!-- iterate through houses and their candies here -->
<% @houses.each do |house| %>
  <ul>
    <li><%=house.address%></li>
    <li>Candies:</li>
    <ul>
      <% house.unclaimed_candies.each do |candy| %>
        <li><%=candy.name%></li>
      <% end %>
    </ul>
  </ul>
<% end %>
